{
  "name": "@nsis/nlf-cli",
  "version": "0.5.1",
  "description": "CLI tool to convert NSIS Language Files to JSON and vice versa",
  "license": "MIT",
  "scripts": {
    "build": "tsc --pretty --project ./",
    "dev": "npm run start",
    "lint": "tslint --project .",
    "start": "tsc --watch --pretty --project ./",
    "test": "ava --verbose"
  },
  "main": "index.js",
  "bin": {
    "nlf": "./index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/node-nlf-cli.git"
  },
  "keywords": [
    "nsis",
    "nsis language file",
    "converter"
  ],
  "dependencies": {
    "@nsis/nlf": "^0.7.0",
    "chromafi": "^2.1.0",
    "commander": "^4.1.1",
    "get-stdin": "^8.0.0",
    "log-symbols": "^4.0.0",
    "update-notifier": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "^14.0.14",
    "ava": "^2.4.0",
    "glob": "^7.1.6",
    "husky": "^4.2.5",
    "tslint": "^6.1.2",
    "typescript": "^3.9.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
