{
  "name": "@nsis/nlf-cli",
  "version": "0.6.1",
  "description": "CLI tool to convert NSIS Language Files to JSON and vice versa",
  "license": "MIT",
  "scripts": {
    "build": "rollup --config",
    "dev": "npm run start",
    "lint": "eslint ./src",
    "start": "rollup --watch --config",
    "test": "ava ./test/default.js --verbose"
  },
  "main": "index.js",
  "bin": {
    "nlf": "index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/node-nlf-cli.git"
  },
  "keywords": [
    "nsis",
    "nsis language file",
    "converter"
  ],
  "dependencies": {
    "@nsis/nlf": "^0.8.1",
    "commander": "^6.2.0",
    "get-stdin": "^8.0.0",
    "log-symbols": "^4.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-typescript": "^6.1.0",
    "@types/node": "^14.0.14",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "ava": "^3.9.0",
    "eslint": "^7.4.0",
    "esm": "^3.2.25",
    "glob": "^7.1.6",
    "husky": "^4.2.5",
    "rollup": "^2.19.0",
    "typescript": "^4.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "ava": {
    "require": [
      "esm"
    ]
  }
}
